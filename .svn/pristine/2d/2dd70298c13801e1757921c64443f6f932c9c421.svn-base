@charset "UTF-8";
html, body, ul, li, button{
	margin: 0;
	padding: 0;
	
}
img{
	display: block;
	border: 0;
}
button {
	border: 0;
	background: none;
}

.btn_fill, .btn-empty {
	border: 1px solid #3367d6;
	padding: 3px 10px;
	text-decoration: none;
	text-align: center;
	border-radius: 3px;
	cursor: pointer;
	box-shadow: 1.5px 1.5px #0520b7;	
	
}
b:hover{
	cursor: pointer;
	color: orange;
}
.btn-fill-small {
	background-color: #e5efff;
	color: #357ff2;
}
.btn-empty {
	background-color: #fff;
	color: #3367d6;
}