@charset "UTF-8";
html, body, ul, li, button{
	margin: 0;
	padding: 0;
	font-family: NanumSquareRoundB;
	
}
@font-face{
	font-family: NanumSquareRoundB;
	src: url(resources/font/NanumSquareRoundB.woff) format('woff');
}
img{
	display: block;
	border: 0;
}
button {
	border: 0;
	background: none;
}

.btn_fill_mh, .btn-empty_mh {
	padding: 3px 10px;
	text-decoration: none;
	text-align: center;
	box-shadow: 1.5px 1.5px #0520b7;
	color: lime;
}
.btn-fill_mh:hover{
	cursor: pointer;
	color: orange;
}
.btn-fill-small {
	background-color: #e5efff;
	color: #357ff2;
}
*:focus { 
	outline:none; 
	}
.main_board_slot{
	border: 2px solid #fff;
	align-content: center;
	width: 1500px;
	height: 220px;
}
.main_board{
	border: 2px solid #848484;
	width: 30%;
	height: 300px;
	left: 30;
	border-radius: 12px;
	
}

.main_board_title{
	margin-left: 10px;
	font-size: large;
	
	
}
.delete{
 margin: 0 auto;
	
}
.mytrpage{
	height: 5%;
}

